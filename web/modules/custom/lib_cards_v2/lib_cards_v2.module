<?php

/**
 * @file
 * Contains lib_cards_v2.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function lib_cards_v2_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the lib_cards_v2 module.
    case 'help.page.lib_cards_v2':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Card with description') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function lib_cards_v2_theme() {
  return [
    'lib_cards_v2' => [
      'render element' => 'children',
    ],
  ];
}
